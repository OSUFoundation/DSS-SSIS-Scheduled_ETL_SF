                  refId="Package\Load Project Unitization Fact\Load Project Unitization Fact\Load Project Unitizations"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation;Microsoft SqlServer v9; (C) 2005 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="Load Project Unitizations"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @ProjectFilter VarChar(100)
DECLARE @StartPeriodDate as DateTime
DECLARE @EndPeriodDate as DateTime
DECLARE @StartPeriod as Int
DECLARE @EndPeriod as Int
SET @StartPeriodDate = '06/30/2008'
SET @StartPeriod = (Select Max(GL7FiscalPeriodsID) + 1 FROM dbo.DIM_Unit_Value)
SET @EndPeriod = @StartPeriod + 2
SET @EndPeriodDate = (SELECT Max(PeriodEndDate) FROM Dim_Date Where GL7FISCALPERIODSID = @EndPeriod)
SET @ProjectFilter = -1		

--Sums up balances by unitization category by unitized period by project--
SELECT 
Sum(NaturalAmount) AS Amount
, TA.AttributeDescription AS UnitizationCategory
, DD.GL7FISCALPERIODSID
, TD.ProjectID
, TD.ProjectDimID
, DD.QtrEndDate AS BalanceDate

FROM dbo.FACT_GLTransactionDistribution TD 

--Join to filter for Unitization accounts only--
	INNER JOIN 
		(
		SELECT A.AccountNumber, AA.AccountSystemID, AttributeDescription, AttributeCategory 
		FROM Dim_AccountAttribute AA INNER JOIN DIM_ACCOUNT A ON A.AccountSystemID = AA.AccountSystemID
		WHERE AttributeCategory = 'UnitizationClass'
		) TA
		ON TD.AccountSystemID = TA.AccountSystemID 

--Join to add date dimension data and filter for quarter ending periods--
	INNER JOIN 
		(
		SELECT GL7FISCALPERIODSID, PeriodEndDate, @EndPeriodDate AS "QtrEndDate" 
		FROM DIM_Date 
		WHERE GL7FISCALPERIODSID &gt;= @StartPeriod and GL7FISCALPERIODSID &lt;= @EndPeriod
		AND MonthEnd = 1
		) DD 
		ON TD.FISCALPERIODSSYSTEMID = DD.GL7FISCALPERIODSID

--Join to filter for only projects in the investment pool--
	INNER JOIN 
		(
		SELECT ProjectDimID 
		FROM DIM_ProjectAttribute 
		WHERE AttributeCategory = 'IncludeInInvestPool' AND AttributeDescription = @ProjectFilter
		) PA 
		ON TD.ProjectDimID = PA.ProjectDimID
--WHERE TD.FundID = '301'

GROUP BY 
TA.AttributeDescription
, DD.GL7FISCALPERIODSID
, TD.ProjectID
, TD.ProjectDimID
, DD.QtrEndDate


UNION
--Join to Add Book Values--
SELECT 
Sum(Balance) AS Amount
, 'Book Value' AS UnitizationCategory
, BV.GL7FISCALPERIODSID
, BV.ProjectID
, BV.ProjectDimID
, BV.BalanceDate

FROM dbo.FACT_BookValue BV

--Filtering only included projects--
INNER JOIN 
(
SELECT ProjectDimID 
FROM DIM_ProjectAttribute 
WHERE  AttributeCategory = 'IncludeInInvestPool' AND AttributeDescription = @ProjectFilter
) PA 
ON BV.ProjectDimID = PA.ProjectDimID

WHERE GL7FISCALPERIODSID is not null AND 
BV.BalanceDate &gt;= @StartPeriodDate

GROUP BY
BV.GL7FISCALPERIODSID
, BV.ProjectID
, BV.ProjectDimID
, BV.BalanceDate

/***
UNION

SELECT 
Sum(Amount)  AS Amount
, 'Book Value' AS UnitizationCategory
, PeriodID
, FE.ProjectID
, ProjectDimID
, '2008-06-30' AS BalanceDate

FROM dbo.FACT_Exclude FE INNER JOIN Dim_Project P ON FE.ProjectID = P.ProjectID

GROUP BY 
 PeriodID
, FE.ProjectID
, ProjectDimID
***/


Order by BalanceDate, UnitizationCategory, ProjectID</property>
