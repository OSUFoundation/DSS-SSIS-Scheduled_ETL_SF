                <component
                  refId="Package\Calculate and Load Project Book Value Balances\Populate Book Value Fact Table\Create Book Value Balances from Project Account Balance Table"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation;Microsoft SqlServer v9; (C) 2005 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="Create Book Value Balances from Project Account Balance Table"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @StartDate as DateTime
DECLARE @EndDate as DateTime 
SET @StartDate = '06/01/2001'
SET @EndDate = GetDate() + 1 

SELECT 
t.ProjectSystemID
, t.ProjectDimID
, t.ProjectID
, t.ProjectDescription 
, t.accountsystemid 
, t.accountnumber 
, t.accountdescription
, t.AccountCategory
, fp.BalanceDate 
, fp.GL7FISCALPERIODSID
, (YEAR(fp.BalanceDate) * 10000) + (Month(fp.BalanceDate) * 100) + (Day(fp.BalanceDate)) AS "BalanceDateDimID" 
, SUM(naturalAmount) AS "Balance"
, 't' as "BalanceType"
, 't' as "BalanceSubType"
, 1 as TransactionCodeDimID

FROM (Select ProjectSystemID
, TD.ProjectDimID
, projectID 
, accountsystemid 
, accountnumber
, accountdescription
, AccountCategory
, projectDescription 
, naturalAmount 
, postdate

FROM  dbo.FACT_GLTransactionDistribution TD
INNER JOIN 
(
SELECT ProjectDimID 
FROM DIM_ProjectAttribute 
WHERE  AttributeCategory = 'IncludeInInvestPool' AND AttributeDescription = -1
) PA 
ON TD.ProjectDimID = PA.ProjectDimID
WHERE AccountCategorySystemID &gt; 2 
and Left(Accountnumber, 3) = '301'
And AccountCode &lt;&gt; '43210'
) t 
CROSS JOIN 
(
SELECT DISTINCT 
ActualDate AS BalanceDate 
, FiscalYear
, FiscalMonth
, FiscalQuarterName
, DateDimID as BalanceDateDimID
, GL7FISCALPERIODSID

FROM DIM_Date 
WHERE ActualDate BETWEEN @StartDate AND @EndDate
AND QtrEnd = 1 

) fp 

WHERE t.PostDate &lt;= fp.balancedate 

GROUP BY 
t.ProjectSystemID
, t.ProjectDimID
, t.projectID 
, t.projectDescription 
, t.accountsystemid 
, t.accountnumber 
, t.accountdescription
, t.AccountCategory
, fp.BalanceDate
, fp.BalanceDateDimID
, fp.GL7FISCALPERIODSID</property>
