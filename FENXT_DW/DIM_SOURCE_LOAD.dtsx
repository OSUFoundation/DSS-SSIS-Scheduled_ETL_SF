              SQLTask:BypassPrepare="False"
              SQLTask:SqlStatementSource="--- Calculates the Last Whole Quarter from Today's Date&#xA;DECLARE @CurrentDate DateTime&#xA;DECLARE @NoQtrs SmallInt&#xA;DECLARE @FiscalYear As Int&#xA;DECLARE @sCurrentYear AS Varchar(4)&#xA;DECLARE @sFiscalMonthDay Varchar(5)&#xA;DECLARE @FiscalEndDate AS DateTime&#xA;DECLARE @FiscalMonthStart AS SmallInt&#xA;DECLARE @LastQtrStartDate DateTime&#xA;SET @CurrentDate=GetDate()&#xA;SET @NoQtrs = (SELECT TOP 1 NoSPPeriods FROM CTL_Config)&#xA;SET @FiscalMonthStart = (SELECT TOP 1 FiscalMonthStart FROM CTL_Config)&#xA;SET @FiscalYear = dbo.BBBI_ReturnFiscalYear (@CurrentDate, @FiscalMonthStart)  &#xA;SET @sCurrentYear =  CAST(@FiscalYear AS VARCHAR(4))&#xA;SET @sFiscalMonthDay = (SELECT TOP 1 FiscalYearEnd FROM CTL_Config)&#xA;SET @FiscalEndDate = CAST(@sCurrentYear + '/' + @sFiscalMonthDay AS DateTime)&#xA;&#xA;-- This Calculates the start date for the first Whole Quarter&#xA;SET  @LastQtrStartDate = &#xA;DATEADD(q, 0, &#xA;CASE &#xA; WHEN @CurrentDate &lt; DATEADD(q, -3, @FiscalEndDate)&#xA;   THEN DATEADD(q,-4,@FiscalEndDate)&#xA; WHEN @CurrentDate &lt; DATEADD(q, -2, @FiscalEndDate)&#xA;   THEN DATEADD(q,-3,@FiscalEndDate)&#xA; WHEN @CurrentDate &lt; DATEADD(q, -1, @FiscalEndDate)&#xA;   THEN DATEADD(q,-2,@FiscalEndDate)&#xA; WHEN @CurrentDate &lt; DATEADD(q, 0, @FiscalEndDate)&#xA;   THEN DATEADD(q,-1,@FiscalEndDate)&#xA; ELSE&#xA;   DATEADD(q,0,@FiscalEndDate)&#xA;END) &#xA;&#xA;--------------------------------------------------------------------------------------------------------------&#xA;UPDATE&#xA;DIM_Source&#xA;SET LastQtrStartDate = @LastQtrStartDate,&#xA;CurrentFiscalEndDate = @FiscalEndDate,&#xA;CurrentFiscalStartDate = DATEADD(yy, -1, @FiscalEndDate) + 1&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
