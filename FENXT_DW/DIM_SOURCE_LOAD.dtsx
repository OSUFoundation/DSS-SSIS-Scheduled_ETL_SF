                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @NoSPPeriods AS SmallInt
DECLARE @CurrentDate DateTime
DECLARE @FiscalYearStart Varchar(10)
DECLARE @LatestFiscalDate DateTime
DECLARE @FiscalMonthStart SmallInt
DECLARE @ClosedYearEndDate DateTime
DECLARE @OpenYearStartDate DateTime
DECLARE @OpenYearFirstPeriodEndDate DateTime
DECLARE @FirstPostDate DateTime
DECLARE @LastPostDate DateTime
DECLARE @OrgName Varchar(100)
DECLARE @LastBeginPeriod DateTime
DECLARE @LastEndPeriod DateTime
DECLARE @CurrentDateDimID Int

SET @NoSPPeriods = 12

SET @OrgName = (SELECT TOP 1 ORGANIZATIONNAME FROM GENERALINFO7)
SET @CurrentDate=GetDate()
SET @CurrentDateDimID = 	(YEAR(@CurrentDate) * 10000) + (Month(@CurrentDate) * 100) + (Day(@CurrentDate))
SET @LatestFiscalDate = (SELECT MAX(STARTDATE) FROM GL7FISCALPERIODS WHERE Sequence = 1)
SET @FiscalMonthStart = Month(@LatestFiscalDate)
SET @FiscalYearStart = Convert(Varchar(5), @LatestFiscalDate, 101)
SET @LastBeginPeriod = DateAdd(mm, -1, Convert(DateTime, Convert(Varchar(4), Year(@CurrentDate)) + '-' + Convert(Varchar(2), Month(@CurrentDate)) + '-01', 101))
-- Last Date for Last Closed Fiscal Year
SET @ClosedYearEndDate = 
(
SELECT MAX(FP.ENDDATE) AS ClosedYearEndDate
FROM   GL7FISCALYEARS FY INNER JOIN
       GL7FISCALPERIODS FP ON FY.GL7FISCALYEARSID = FP.GL7FISCALYEARSID
WHERE  (FY.STATUS &lt;&gt; 1)
)
-- First Date of First Open Fiscal Year
SET @OpenYearStartDate = 
(
SELECT MIN(FP.STARTDATE) AS OpenYearStartDate
FROM   GL7FISCALYEARS FY INNER JOIN
       GL7FISCALPERIODS FP ON FY.GL7FISCALYEARSID = FP.GL7FISCALYEARSID
WHERE  (FY.STATUS = 1)
)
-- Date of the End of the First Period of the First Open Fiscal Year
SET @OpenYearFirstPeriodEndDate = 
(
SELECT MAX(FP.ENDDATE) AS OpenYearFirstPeriodEndDate 
FROM   GL7FISCALYEARS FY INNER JOIN
       GL7FISCALPERIODS FP ON FY.GL7FISCALYEARSID = FP.GL7FISCALYEARSID
WHERE  (FY.STATUS = 1)
)
SET @FirstPostDate = (SELECT MIN(POSTDATE) FROM GL7TRANSACTIONS)
SET @LastPostDate = (SELECT MAX(POSTDATE) FROM GL7TRANSACTIONS)


SELECT 
@OrgName AS OrganizationName,
@LatestFiscalDate AS LatestFiscalDate,
@FiscalMonthStart AS FiscalMonthStart,
LEFT(CONVERT(Varchar(10), MAX(ENDDATE) + 1,101), 5) AS FiscalYearStart, 
