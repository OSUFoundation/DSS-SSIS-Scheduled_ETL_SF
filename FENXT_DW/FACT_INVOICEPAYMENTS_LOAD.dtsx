                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT     
i.AP7INVOICESID AS InvoiceSystemID, 
i.AP7VENDORSID VendorSystemID, 
i.INVOICENUMBER InvoiceNumber, 
p.AP7PAYMENTSID PaymentSystemID, 
c.CHECKDATE CheckDate, 
CASE c.CHECKTYPE 
WHEN 1 THEN 'Computer check'
WHEN 2THEN 'Manual check'
WHEN 4 THEN 'One-time check'
WHEN 8 THEN 'EFT check'
WHEN 16 THEN 'Bank draft'
WHEN 32 THEN 'Deposit'
WHEN 65 THEN 'Adj trans out'
WHEN 66 THEN 'Adj trans in'
WHEN 67THEN 'Adj payment'
WHEN 68 THEN 'Adj deposit'
ELSE '' END AS CheckType,
c.CHECKNUMBER AS CheckNumber, 
c.CHECKNOTES AS CheckNotes,
c.PAYEENAME AS PayeeName, 
c.AMOUNT AS Amount, 
c.CONTACTNAME AS ContactName, 
CASE c.CLEARED WHEN -1 THEN 'Y' ELSE 'N' END AS IsCleared, 
c.CLEAREDDATE AS ClearedDate, 
c.POSTSTATUS AS PostStatusSystemID, 
c.POSTDATE AS PostDate, 
CASE c.VOIDED WHEN -1 THEN 'Y' ELSE 'N' END AS IsVoided, 
c.VOIDEDDATE AS VoidedDate, 
CASE c.RECONCILED WHEN -1 THEN 'Y' ELSE 'N' END AS IsReconciled, 
c.RECONCILEDDATE AS ReconciledDate,
i.BANKSID AS BankSystemID,
b.ACCOUNTNUMBER AS AccountNumber,
b.BANKNAME AS BankName
FROM         
AP7PAYMENTS AS p 
INNER JOIN AP7INVOICES AS i ON p.AP7INVOICESID = i.AP7INVOICESID 
INNER JOIN CHECKS7 AS c ON p.CHECKS7ID = c.CHECKS7ID
INNER JOIN BANKS AS b ON c.BANKSID = b.BANKSID</property>
