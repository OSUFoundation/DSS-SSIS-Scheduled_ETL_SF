                  description="OLE DB Source"
                  name="Create Project Summary Balances from Project Account Balance Table"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @StartDate as DateTime
DECLARE @EndDate as DateTime 
SET @StartDate = '09/30/1997'
--(SELECT OpenYearStartDate FROM UNR_CTL_Config) 
SET @EndDate = GetDate() + 1 

SELECT 
t.ProjectSystemID
, t.ProjectDimID
, t.ProjectID
, t.ProjectDescription 
, t.accountsystemid 
, t.accountnumber 
, t.accountdescription
, t.AccountCategory
, fp.BalanceDate 
, (YEAR(fp.BalanceDate) * 10000) + (Month(fp.BalanceDate) * 100) + (Day(fp.BalanceDate)) AS "BalanceDateDimID" 
, SUM(naturalAmount) AS "Balance"
, 't' as "BalanceType"
, 't' as "BalanceSubType"
, 1 as TransactionCodeDimID

FROM (Select ProjectSystemID
, ProjectDimID
, projectID 
, accountsystemid 
, accountnumber
, accountdescription
, AccountCategory
, projectDescription 
, naturalAmount 
, postdate

FROM  dbo.FACT_GLTransactionDistribution
WHERE substring(accountnumber, 4, 5) IN('17000', '17100','17200', '17300', '17400')
) t 
CROSS JOIN 
(
SELECT DISTINCT 
ActualDate AS BalanceDate 
, FiscalYear
, FiscalMonth
, FiscalQuarterName
, DateDimID as BalanceDateDimID
FROM DIM_Date 
WHERE ActualDate BETWEEN @StartDate AND @EndDate
AND MonthEnd = 1
) fp 

WHERE t.PostDate &lt;= fp.balancedate 

GROUP BY 
t.ProjectSystemID
, t.ProjectDimID
, t.projectID 
, t.projectDescription 
, t.accountsystemid 
, t.accountnumber 
, t.accountdescription
, t.AccountCategory
, fp.BalanceDate
, fp.BalanceDateDimID</property>
