                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
ProjectSystemID, ProjectDimID,
AccountSystemID, AccountDimID,
BalanceDate, 
TransactionCode1DimID, 
TransactionCode2DimID, 
TransactionCode3DimID, 
TransactionCode4DimID, 
TransactionCode5DimID,
2 AS TransactionTypeID,
'Credit' AS TransactionType,
SUM(BeginningBalance)* -1 AS Balance
FROM
(
SELECT     
S.ProjectSystemID, S.ProjectDimID, S.AccountSystemID, S.AccountDimID, S.StartDate AS BalanceDate, 
TransactionCode1DimID, 
TransactionCode2DimID, 
TransactionCode3DimID, 
TransactionCode4DimID, 
TransactionCode5DimID,
S.BeginningBalance
FROM         
TEMP_GLSummary S INNER JOIN
(SELECT    ProjectSystemID, ProjectDimID, AccountSystemID, MIN(StartDate) AS BalanceDate
FROM          TEMP_GLSummary S
WHERE      (FiscalCloseStatus = 'Open') AND (AccountCategoryCode &gt; 2)
GROUP BY ProjectSystemId, ProjectDimID, AccountSystemID, AccountDimID) BD 
ON S.ProjectSystemId = BD.ProjectSystemId 
AND S.AccountSystemID = BD.AccountSystemID 
AND S.StartDate = BD.BalanceDate
) T
GROUP BY
ProjectSystemId, ProjectDimID, AccountSystemID, AccountDimID, BalanceDate,
TransactionCode1DimID, 
TransactionCode2DimID, 
TransactionCode3DimID, 
TransactionCode4DimID, 
TransactionCode5DimID</property>
