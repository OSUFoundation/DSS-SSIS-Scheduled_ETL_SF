                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
B.GL7SUMMARYID,    
B.GL7PROJECTSID AS ProjectSystemID, 
--P.PROJECTID AS ProjectID, 
B.GL7ACCOUNTSID AS AccountSystemID,
--A.ACCOUNTNUMBER AS AccountNumber,
--A.CATEGORY AS AccountCategoryCode, 
B.BEGINNINGBALANCE AS BeginningBalance, 
CASE B.STATUS WHEN 1 THEN 'Pre-close' WHEN 2 THEN 'Close' WHEN 3 THEN 'Open' ELSE '' END AS CloseStatus, 
B.CREDIT AS Credit, B.DEBIT AS Debit, 
B.GL7TRANSACTIONCODES1 AS TCode1, 
B.GL7TRANSACTIONCODES2 AS TCode2, 
B.GL7TRANSACTIONCODES3 AS TCode3, 
B.GL7TRANSACTIONCODES4 AS TCode4, 
B.GL7TRANSACTIONCODES5 AS TCode5, 
B.CLASS AS Class, 
FY.YEARID AS YearID, 
FY.DESCRIPTION AS YearDescription, 
CASE FY.STATUS WHEN 1 THEN 'Open' WHEN 2 THEN 'Pre-closed'
WHEN 3 THEN 'Closed' WHEN 4 THEN 'Purged' WHEN 5 THEN 'Soft-closed' ELSE '' END
AS FiscalCloseStatus, 
FY.SUMMARIZED AS FiscalSummarized, 
FY.FISCALPERIODS AS FiscalPeriods, 
FY.SEQUENCE AS FiscalYearSequence,
FP.SEQUENCE AS FiscalPeriodSequence,
CASE FP.CLOSED WHEN -1 THEN 1 WHEN 0 THEN 0 END AS FiscalPeriodClosed,
FP.STARTDATE AS StartDate,
FP.ENDDATE AS EndDate,
(YEAR(FP.STARTDATE) * 10000) + (Month(FP.STARTDATE) * 100) + (Day(FP.STARTDATE)) AS StartDateDimID

FROM         
GL7SUMMARY B 
--INNER JOIN GL7PROJECTS P ON B.GL7PROJECTSID = P.GL7PROJECTSID 
--INNER JOIN GL7ACCOUNTS A ON B.GL7ACCOUNTSID = A.GL7ACCOUNTSID 
INNER JOIN GL7FISCALPERIODS FP ON B.GL7FISCALPERIODSID = FP.GL7FISCALPERIODSID 
INNER JOIN GL7FISCALYEARS FY ON FP.GL7FISCALYEARSID = FY.GL7FISCALYEARSID</property>
