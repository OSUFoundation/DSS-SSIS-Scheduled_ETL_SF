                  refId="Package\Calculate and Load Project Balances\Populate Project Account Balance Fact Table\Calculate Periodic Balances from Feeder Tables"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation;Microsoft SqlServer v9; (C) 2005 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="Calculate Periodic Balances from Feeder Tables"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @StartDate DateTime
DECLARE @EndDate DateTime
SET @StartDate = (SELECT TOP 1 OpenYearStartDate FROM DIM_Source)
SET @EndDate = GetDate() + 1

SELECT 
ISNULL(td.ProjectID, bs.ProjectID) AS ProjectID,
ISNULL(td.ProjectDimID, bs.ProjectDimID) AS ProjectDimID,
ISNULL(td.ProjectSystemID, bs.ProjectSystemID) AS ProjectSystemID,
ISNULL(td.AccountDimID, bs.AccountDimID) AS AccountDimID,
ISNULL(td.TransactionCodeDimID, bs.TransactionCodeDimID) AS TransactionCodeDimID,
--'Fund Balance' AS BalanceSubType,
--'T' AS BalanceType,
ISNULL(td.BalanceDate, bs.BalanceDate) AS BalanceDate,
ISNULL(td.BalanceDateDimID, bs.BalanceDateDimID) AS BalanceDateDimID,
ISNULL(td.Balance, bs.Balance) AS Balance
FROM
(
SELECT 
t.ProjectID,
t.ProjectDimID,
t.ProjectSystemID,
t.AccountDimID,
t.TransactionCode1DimID AS TransactionCodeDimID,
fp.BalanceDate,
(YEAR(fp.BalanceDate) * 10000) + (Month(fp.BalanceDate) * 100) + (Day(fp.BalanceDate)) AS BalanceDateDimID,
SUM(Amount) AS Balance
FROM FACT_GLTransactionDistribution t
CROSS JOIN (SELECT DISTINCT PeriodStartDate AS BalanceDate , FiscalYear, FiscalMonth, FiscalQuarterName 
		FROM DIM_Date WHERE ActualDate BETWEEN @StartDate AND @EndDate) fp
WHERE 
	t.AccountCategorySystemID &gt; 2
	AND t.PostDate &lt;= fp.BalanceDate
	AND t.ProjectSystemID IS NOT NULL AND t.ProjectSystemID &lt;&gt; -1

GROUP BY t.ProjectID, t.ProjectDimID, t.ProjectSystemID, 
fp.BalanceDate, (YEAR(fp.BalanceDate) * 10000) + (Month(fp.BalanceDate) * 100) + (Day(fp.BalanceDate)), AccountDimID
,t.TransactionCode1DimID
) td
FULL OUTER JOIN
(
SELECT 
s.ProjectID, 
s.ProjectDimID,
s.ProjectSystemID,
s.AccountDimID,
s.TransactionCode1DimID AS TransactionCodeDimID,
s.StartDate AS BalanceDate,
s.StartDateDimID AS BalanceDateDimID,
(SUM(s.BeginningBalance) * -1) AS Balance
FROM TEMP_GLSummary s
WHERE 
s.AccountCategoryCode &gt; 2
AND s.FiscalCloseStatus = 'Closed'
AND s.ProjectSystemID IS NOT NULL AND s.ProjectSystemID &lt;&gt; -1
GROUP BY
s.ProjectID, s.ProjectDimID, s.ProjectSystemID, s.StartDate, s.StartDateDimID, s.AccountDimID
, s.TransactionCode1DimID
) bs
ON td.ProjectDimID = bs.ProjectDimID
AND td.BalanceDateDimID = bs.BalanceDateDimID</property>
