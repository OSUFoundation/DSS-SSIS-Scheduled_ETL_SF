            <component
              refId="Package\Campaign Member\ODBC Source"
              componentClassID="Attunity.SSISODBCSrc"
              description="ODBC Source"
              name="ODBC Source"
              usesDispositions="true"
              version="6">
              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
    c.ID                                    AS ContactId   
    ,cd.Prospect_ID
    ,CAST
        (
            CONCAT
              (
                  'P' 
                  ,CASE
                      WHEN MONTH(CAST(CONCAT(cd.Date_Time, ':00') AS DATE)) BETWEEN 7 AND 12 THEN (YEAR(CAST(CONCAT(cd.Date_Time, ':00') AS DATE)) % 100) + 1
                      ELSE YEAR(CAST(CONCAT(cd.Date_Time, ':00') AS DATE)) % 100   //YEAR(GetDate())
                      END
                  ,'All'
              )                                                   
        AS VARCHAR(20))                     AS ucinn_ascendv2__Motivation_Code
    
    ,CASE
        WHEN cd.Complete_Y_Or_N_ = 'Y' THEN 'Calls Completed'
        ELSE 'Calls Incomplete'
        END                                 AS Call_Status
    ,cd.Segment_Name                           AS Segment
    //,Donor_Type                              AS Donor_Type
    ,'1 Year Lapsed'                        AS Donor_Type
    ,cd.Call_Result                            AS Call_Result
    ,CONCAT(cd.Date_Time, ':00')               AS Appeal_Date
    ,CASE WHEN Prospect_ID = '191'
        THEN '' 
        ELSE '0038A00000TvYYSQA3'                             END AS Cowboy_Caller
  
FROM
    "CALL_CENTER"."WB_DC"."CALL_DETAILS" cd
        INNER JOIN "ASCEND_QA_FIVETRAN"."ASCEND_QA"."CONTACT" c ON cd.Prospect_ID = c.UCINN_ASCENDV_2_DONOR_ID_C
        //INNER JOIN "ASCEND_QA_FIVETRAN"."ASCEND_QA"."CONTACT" cc ON '00000359' = c.UCINN_ASCENDV_2_DONOR_ID_C
WHERE
    cd.Ascend_Load_Date IS NULL</property>
