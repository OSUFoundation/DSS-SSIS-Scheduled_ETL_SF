                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
    //cd.Prospect_ID                              AS DonorID
    'AG' AS Name
    ,CONCAT
        (
            'Annual Giving Campaign '
            ,CASE
                WHEN MONTH(GetDate()) BETWEEN 7 AND 12 THEN YEAR(GetDate()) + 1
                ELSE YEAR(GetDate())
                END
        )                                       AS Appeal_Name
    ,CONCAT
        (
            'AG' 
            ,CASE
                WHEN MONTH(GetDate()) BETWEEN 7 AND 12 THEN YEAR(GetDate()) + 1
                ELSE YEAR(GetDate())
                END
        )                                       AS ucinn_ascendv2__Motivation_Code        
    //,'Oklahoma State University Foundation'     AS Originating_Partner
    ,'a208A000000nQirQAE'                       AS Originating_Partner
    ,'In Progress'                              AS Status
    ,'Appeal Campaign'                          AS Type
    ,1                                          AS IsActive
    ,CONCAT
        (
            'Annual Giving Campaign for Fiscal Year '
            ,CASE
                WHEN MONTH(GetDate()) BETWEEN 7 AND 12 THEN YEAR(GetDate()) + 1
                ELSE YEAR(GetDate())
                END
        )                                       AS Description
    
    ,CONCAT
        (
            '7/1/' 
            ,CASE
                WHEN MONTH(GetDate()) BETWEEN 7 AND 12 THEN YEAR(GetDate())
                ELSE YEAR(GetDate()) - 1
                END
        )                                       AS StartDate
    ,CONCAT
        (
            '6/30/' 
            ,CASE
                WHEN MONTH(GetDate()) BETWEEN 7 AND 12 THEN YEAR(GetDate()) + 1
                ELSE YEAR(GetDate())
                END
        )                                       AS EndDate</property>
