            <component
              refId="Package\Create Appeal 1\Get New Campaign data"
              componentClassID="Attunity.SSISODBCSrc"
              description="ODBC Source"
              name="Get New Campaign data"
              usesDispositions="true"
              version="6">
              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
    CONCAT
        (
            'AG' 
            ,CASE
                WHEN MONTH(GetDate()) BETWEEN 5 AND 12 THEN YEAR(GetDate()) + 1
                ELSE YEAR(GetDate())
                END
        )                                       AS Name
    ,CONCAT
        (
            'Cowboy Caller '
            ,CASE
                WHEN MONTH(GetDate()) BETWEEN 5 AND 12 THEN YEAR(GetDate()) + 1
                ELSE YEAR(GetDate())
                END
        )                                       AS Appeal_Name
    ,CONCAT
        (
            'P' 
            ,CASE
                WHEN MONTH(GetDate()) BETWEEN 5 AND 12 THEN (YEAR(GETDATE()) % 100) + 1
                ELSE YEAR(GETDATE()) % 100   //YEAR(GetDate())
                END
            ,'All'
        )                                       AS ucinn_ascendv2__Motivation_Code            
    //,'Oklahoma State University Foundation'     AS Originating_Partner
    ,'a208A000000nQirQAE'                       AS Originating_Partner
    ,'In Progress'                              AS Status
    ,'Appeal'                                     AS Type
    ,1                                          AS IsActive
    ,CONCAT
        (
            'Annual Giving Cowboy Caller for Fiscal Year '
            ,CASE
                WHEN MONTH(GetDate()) BETWEEN 5 AND 12 THEN YEAR(GetDate()) + 1
                ELSE YEAR(GetDate())
                END
        )                                       AS Description
    
    ,CONCAT
        (
            '7/1/' 
            ,CASE
                WHEN MONTH(GetDate()) BETWEEN 5 AND 12 THEN YEAR(GetDate())
                ELSE YEAR(GetDate()) - 1
                END
        )                                       AS StartDate
    ,CONCAT
        (
            '6/30/' 
            ,CASE
                WHEN MONTH(GetDate()) BETWEEN 5 AND 12 THEN YEAR(GetDate()) + 1
                ELSE YEAR(GetDate())
                END
        )                                       AS EndDate
     ,CAST(CONCAT
        (
            'AG' 
            ,CASE
                WHEN MONTH(GetDate()) BETWEEN 5 AND 12 THEN YEAR(GetDate()) + 1
                ELSE YEAR(GetDate())
                END
        ) AS VARCHAR(50)) AS ParentMotivationCode</property>
