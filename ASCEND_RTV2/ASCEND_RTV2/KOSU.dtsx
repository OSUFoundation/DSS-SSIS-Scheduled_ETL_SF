		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
            string allegianceFile = (string)Dts.Variables["CurrentAllegianceFile"].Value;
            string creditFile = (string)Dts.Variables["CurrentCreditFile"].Value;
            bool fireAgain = true;

            Dts.Events.FireInformation(0, "Script Task", creditFile, null, 0, ref fireAgain);

            //List variables for storing reference #'s for comparison.
            List <string> allegianceRefNumbers = new List<string>();
            List <string> creditRefNumbers = new List<string>();

            //Validate Allegiance file and get reference numbers
            using (TextFieldParser parser = new TextFieldParser(allegianceFile))
            {
                parser.TextFieldType = FieldType.Delimited;
                parser.SetDelimiters(",");
                while (!parser.EndOfData)
                {
                    string[] fields = parser.ReadFields();
                    if(parser.LineNumber == 2 && !fields[6].Equals("Reference #"))
                    {
                        Dts.Events.FireInformation(0, "Script Task", "Allegiance File failed column validation.", null, 0, ref fireAgain);
                        Dts.TaskResult = (int)ScriptResults.Failure;
                        return;
                    }

                    else if(parser.LineNumber != 2)
                    {
                        allegianceRefNumbers.Add(fields[6]);
                    }
                }
                parser.Close();
            }
            Dts.Events.FireInformation(0, "Script Task", "Finished Allegiance file.", null, 0, ref fireAgain);

            using (TextFieldParser parser2 = new TextFieldParser(creditFile))
            {
                parser2.TextFieldType = FieldType.Delimited;
                parser2.SetDelimiters(",");
                while (!parser2.EndOfData)
                {
                    string[] fields = parser2.ReadFields();
                    if(parser2.LineNumber == 2 && !fields[26].Equals("reference")) {
                        Dts.Events.FireInformation(0, "Script Task", "Credit File failed validation.", null, 0, ref fireAgain);
                        Dts.TaskResult = (int)ScriptResults.Failure;
                        return;
                    }
                    else if(parser2.LineNumber != 2)
                    {
                        Dts.Events.FireInformation(0, "Script Task", fields[26], null, 0, ref fireAgain);
                        creditRefNumbers.Add(fields[26]);
                    }

                }
                parser2.Close();
            }

            if(allegianceRefNumbers.All(creditRefNumbers.Contains))
            {
                Dts.Variables["Matched"].Value = true;
                Dts.Events.FireInformation(0, "Script Task", "Found file match.", null, 0, ref fireAgain);
            }

            Dts.TaskResult = (int)ScriptResults.Success;
		}

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
}]]></ProjectItem>
